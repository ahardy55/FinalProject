<div class="logInPage">
<h1>LOG IN</h1>

<span><%= form_tag sessions_path do %>
  <p>
  	<%= label_tag :email %><br />
  	<%= text_field_tag :email, params[:email] %>
  </p>
  <p>
  <%= label_tag :password %><br \>
  <%= password_field_tag :password %>
  </p>
  <%= submit_tag "Log in", :class => 'button' %></span>
</div>
<% end %>

